  <title>saloon</title>


  <%- include('../../partials/headlink.ejs') %>
</head>

<body>
  <%- include('../../partials/alertMsg.ejs') %>
    <%- include('../../partials/Header.ejs') %>
      <%- include("../../partials/sidebar.ejs") %>

        <main id="main" class="main">
          <div class="pagetitle">
            <h1>
              <%-user.type %>
            </h1>
            <nav>
              <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="/">Home</a></li>
                <li class="breadcrumb-item active">Dashboard</li>
              </ol>
            </nav>
          </div><!-- End Page Title -->

          <section class="section dashboard">
            <div class="row">
              <div class="row">
                <div class="col-lg-12">

                  <div class="card">
                    <div class="card-body">
                      <!-- //1. add sallon register step -1 -->
                      <% if(typeof data !=="undefined" && data){%>
                        <h5 class="card-title">Edit Saloon</h5>
                        
                          <form class="needs-validation" action="/add_saloon_store?id=<%=data._id%>" method="post"
                            novalidate>
                            <%}else{%>
                              <h5 class="card-title">Add Saloon</h5>
                              <form class="needs-validation" action="/add_saloon_store" method="post" novalidate>
                                <%}%>
                                  <div class="row g-3">
                                    <div class="col">
                                      <label for="">Store Name</label><br>
                                      <input type="text" maxlength="30" name="storeName" class="form-control"
                                        placeholder="Enter Your Store Name"
                                        value="<%=data?.storeName!=undefined ? data?.storeName : "" %>" required>
                                      <div class="invalid-feedback">Please Enter Store Name.</div>

                                    </div>
                                    <div class="col">
                                      <label for="">Store Email</label><br>
                                      <input required type="email" name="email" class="form-control"
                                        placeholder="Enter Store email"
                                        value="<%=data?.email!=undefined ? data?.email : "" %>"
                                        <%=data?.email!=undefined ? "readonly" : "" %>>
                                      <div class="invalid-feedback">Please Enter Store Email.</div>

                                    </div>

                                  </div><br>

                                  <% if(typeof data !=="undefined" && data){%>
                                    <div class="row g-3">
                                      <div class="col">
                                        <label for="">Password</label><br>
                                        <input required type="password" minlength="6" maxlength="8" name="password"
                                          class="form-control" placeholder="Enter a Password"
                                          value="<%=data?.password!=undefined ? data?.password : "" %>">
                                        <div class="invalid-feedback">Please Enter Password.</div>

                                      </div>

                                      <div class="col">
                                        <label for="">Phone</label><br>
                                        <input type="number" maxlength="10" id="Phone" name="Phone"
                                          class="form-control pac-target-input" placeholder="Enter Your Mobile Number"
                                          value="<%=data?.Phone!=undefined ? data?.Phone : "" %>" readonly required>
                                        <div class="invalid-feedback">Please Enter Phone.</div>

                                      </div>
                                    </div>
                                    <%}else{%>
                                      <div class="row g-3">
                                        <div class="col">
                                          <label for="">Password</label><br>
                                          <input required type="password" minlength="6" maxlength="8" name="password"
                                            class="form-control" placeholder="Enter Password"
                                            value="<%=data?.password!=undefined ? data?.password : "" %>">
                                          <div class="invalid-feedback">Please Enter Password.</div>

                                        </div>
                                        <div class="col">
                                          <label for="">Confirm Password</label><br>
                                          <input required type="password" minlength="6" maxlength="8"
                                            name="confromPassword" class="form-control"
                                            placeholder="Enter confrom Password"
                                            value="<%=data?.password!=undefined ? data?.password : "" %>">
                                          <div class="invalid-feedback">Please Enter Confirm Password.</div>

                                        </div>
                                      </div><br>
                                      <div class="row g-3">
                                        <div class="col">
                                          <label for="">Phone</label><br>
                                          <input type="number"
                                            onKeyDown="if(this.value.length==10 && event.keyCode!=8) return false;"
