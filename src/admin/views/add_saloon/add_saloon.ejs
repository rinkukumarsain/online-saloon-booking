<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>view-saloon</title>
    <%- include('../../partials/headlink.ejs') %>
</head>

<body>
   
        <%- include('../../partials/Header.ejs') %>
            <%- include("../../partials/sidebar.ejs") %>

                <main id="main" class="main">
                    <div class="pagetitle">
                        <h1>
                            <%-user.type %>
                        </h1>
                        <nav>
                            <ol class="breadcrumb">
                                <li class="breadcrumb-item"><a href="/">Home</a></li>
                                <li class="breadcrumb-item active">Dashboard</li>
                            </ol>
                        </nav>
                    </div><!-- End Page Title -->

                    <section class="section dashboard">
                        <div class="row">
                          <div class="row">
                            <div class="col-lg-12">
                              <div class="card">
                                <div class="card-body">
                                  <h5 class="card-title">Add Saloon</h5>
                                  
                                      <% if(typeof saloon_data !=="undefined" && saloon_data){%>
                                      <form action="/add_saloon_store" method="post" enctype="multipart/form-data">
                                        <div class="row mb-3">
                                          <label for="inputText" class="col-sm-2 col-form-label">Store Name</label>
                                          <div class="col-sm-10">
                                            <input required type="text" name="storeName" class="form-control" value="<%= saloon_data.name%>" placeholder="Enter a Store Name">
                                          </div>
                                        </div>
                                        
                                        <div class="row mb-3">
                                            <label class="col-sm-2 col-form-label">Multi Select</label>
                                            <div class="col-sm-10">
                                              <select class="form-select" multiple="" name="category[]"aria-label="multiple select example">
                                                <option selected="">Open this select menu</option>
                                                <option value="spa">Spa</option>
                                                <option value="parlour">Parlour</option>
                                              </select>
                                            </div>
                                          </div>
                    
                    
                                        <div class="row mb-3">
                                          <label for="inputNumber" class="col-sm-2 col-form-label">Image</label>
                                          <div class="col-sm-10">
                                            <input required class="form-control" type="file" name="image" id="formFile" multiple>
                                          </div>
                                        </div>
                    
                                        <div class="row mb-3">
                                          <label for="inputText" class="col-sm-2 col-form-label">Email</label>
                                          <div class="col-sm-10">
                                            <input required type="text" name="Email" value="<%= saloon_data.Email%>" class="form-control" placeholder="Enter a Email">
                                          </div>
                                        </div>
                    
                                        <div class="row mb-3">
                                          <label for="inputText" class="col-sm-2 col-form-label">Password</label>
                                          <div class="col-sm-10">
                                            <input required type="text" name="password" class="form-control" placeholder="Enter a Password">
                                          </div>
                                        </div>
                  
                                        <div class="row mb-3">
                                          <label for="inputText" class="col-sm-2 col-form-label">Number</label>
                                          <div class="col-sm-10">
                                            <input required type="number" id="addressSearch" name="PhoneNumber" value="<%= saloon_data.PhoneNumber%>" class="form-control pac-target-input" placeholder="Enter a number" autocomplete="off">
                                          </div>
                                        </div>

                                        <div class="row mb-3">
                                            <label for="inputText" class="col-sm-2 col-form-label">Shop Number</label>
                                            <div class="col-sm-10">
                                              <input required type="number" id="addressSearch" name="shopNumber" value="<%= saloon_data.shopNumber%>" class="form-control pac-target-input" placeholder="Enter a shopNumber" autocomplete="off">
                                            </div>
                                          </div>
                                        
                                          <div class="row mb-3">
                                            <label for="inputText" class="col-sm-2 col-form-label">Pin Code</label>
                                            <div class="col-sm-10">
                                              <input required type="text" id="addressSearch" name="pincode"  value="<%= saloon_data.pincode%>"class="form-control pac-target-input"  placeholder="Enter a Pin Code" autocomplete="off">
                                            </div>
                                          </div>

                                          <div class="row mb-3">
                                            <label for="inputText" class="col-sm-2 col-form-label">city</label>
                                            <div class="col-sm-10">
                                              <input required type="text" id="addressSearch" name="city"  value="<%= saloon_data.city%>"class="form-control pac-target-input" placeholder="Enter a city"  autocomplete="off">
                                            </div>
                                          </div>

                                          <div class="row mb-3">
                                            <label for="inputText" class="col-sm-2 col-form-label">State</label>
                                            <div class="col-sm-10">
                                              <input required type="text" id="addressSearch" name="state" value="<%= saloon_data.state%>" class="form-control pac-target-input" placeholder="Enter a state" autocomplete="off">
                                            </div>
                                          </div>

                                          <div class="row mb-3">
                                            <label for="inputText" class="col-sm-2 col-form-label">Aria</label>
                                            <div class="col-sm-10">
                                              <input required type="text" id="addressSearch" name="aria"  value="<%= saloon_data.aria%>"class="form-control pac-target-input" placeholder="Enter a Aria" autocomplete="off">
                                            </div>
                                          </div>

                                          
                                          <div class="row mb-3">
                                            <label for="inputText" class="col-sm-2 col-form-label">Type</label>
                                            <div class="col-sm-10">
                                              <input required type="text" id="addressSearch" name="type" value="<%= saloon_data.type%>" class="form-control pac-target-input" placeholder="saloon type" autocomplete="off">
                                            </div>
                                          </div>

                                          <div class="row mb-3">
                                            <label for="inputPassword" class="col-sm-2 col-form-label">Desciption</label>
                                            <div class="col-sm-10">
                                              <textarea class="form-control" name="description" style="height: 100px"><%= saloon_data.description%></textarea>
                                            </div>
                                          </div>
                                        
                                        <div class="row mb-3">
                                          <label class="col-sm-2 col-form-label">Submit Button</label>
                                          <div class="col-sm-10">
                                            <button type="submit" class="btn btn-primary">Submit</button>
                                          </div>
                                        </div>
                        
                                      </form>
                                      <%}else{%>
                                        <form action="/add_saloon_store" method="post" enctype="multipart/form-data">
                                            <div class="row mb-3">
                                              <label for="inputText" class="col-sm-2 col-form-label">Store Name</label>
                                              <div class="col-sm-10">
                                                <input required type="text" name="storeName" class="form-control" placeholder="Enter a Store Name">
                                              </div>
                                            </div>
                                            
                                            <div class="row mb-3">
                                                <label class="col-sm-2 col-form-label">Multi Select</label>
                                                <div class="col-sm-10">
                                                  <select class="form-select" multiple="" name="category[]"aria-label="multiple select example">
                                                    <option selected="">Open this select menu</option>
                                                    <option value="spa">Spa</option>
                                                    <option value="parlour">Parlour</option>
                                                  </select>
                                                </div>
                                              </div>
                        
                        
                                            <div class="row mb-3">
                                              <label for="inputNumber" class="col-sm-2 col-form-label">Image</label>
                                              <div class="col-sm-10">
                                                <input required class="form-control" type="file" name="image" id="formFile" multiple>
                                              </div>
                                            </div>
                        
                                            <div class="row mb-3">
                                              <label for="inputText" class="col-sm-2 col-form-label">Email</label>
                                              <div class="col-sm-10">
                                                <input required type="text" name="Email" class="form-control" placeholder="Enter a Email">
                                              </div>
                                            </div>
                        
                                            <div class="row mb-3">
                                              <label for="inputText" class="col-sm-2 col-form-label">Password</label>
                                              <div class="col-sm-10">
                                                <input required type="text" name="password" class="form-control" placeholder="Enter a Password">
                                              </div>
                                            </div>
                      
                                            <div class="row mb-3">
                                              <label for="inputText" class="col-sm-2 col-form-label">Number</label>
                                              <div class="col-sm-10">
                                                <input required type="number" id="addressSearch" name="PhoneNumber" class="form-control pac-target-input" placeholder="Enter a number" autocomplete="off">
                                              </div>
                                            </div>
    
                                            <div class="row mb-3">
                                                <label for="inputText" class="col-sm-2 col-form-label">Shop Number</label>
                                                <div class="col-sm-10">
                                                  <input required type="number" id="addressSearch" name="shopNumber" class="form-control pac-target-input" placeholder="Enter a shopNumber" autocomplete="off">
                                                </div>
                                              </div>
                                            
                                              <div class="row mb-3">
                                                <label for="inputText" class="col-sm-2 col-form-label">Pin Code</label>
                                                <div class="col-sm-10">
                                                  <input required type="text" id="addressSearch" name="pincode" class="form-control pac-target-input"  placeholder="Enter a Pin Code" autocomplete="off">
                                                </div>
                                              </div>
    
                                              <div class="row mb-3">
                                                <label for="inputText" class="col-sm-2 col-form-label">city</label>
                                                <div class="col-sm-10">
                                                  <input required type="text" id="addressSearch" name="city" class="form-control pac-target-input" placeholder="Enter a city"  autocomplete="off">
                                                </div>
                                              </div>
    
                                              <div class="row mb-3">
                                                <label for="inputText" class="col-sm-2 col-form-label">State</label>
                                                <div class="col-sm-10">
                                                  <input required type="text" id="addressSearch" name="state" class="form-control pac-target-input" placeholder="Enter a state" autocomplete="off">
                                                </div>
                                              </div>
    
                                              <div class="row mb-3">
                                                <label for="inputText" class="col-sm-2 col-form-label">Aria</label>
                                                <div class="col-sm-10">
                                                  <input required type="text" id="addressSearch" name="aria" class="form-control pac-target-input" placeholder="Enter a Aria" autocomplete="off">
                                                </div>
                                              </div>
    
                                              
                                              <div class="row mb-3">
                                                <label for="inputText" class="col-sm-2 col-form-label">Type</label>
                                                <div class="col-sm-10">
                                                  <input required type="text" id="addressSearch" name="type" class="form-control pac-target-input" placeholder="saloon type" autocomplete="off">
                                                </div>
                                              </div>
    
                                              <div class="row mb-3">
                                                <label for="inputPassword" class="col-sm-2 col-form-label">Desciption</label>
                                                <div class="col-sm-10">
                                                  <textarea class="form-control" name="description" style="height: 100px"></textarea>
                                                </div>
                                              </div>
                                            
                                            <div class="row mb-3">
                                              <label class="col-sm-2 col-form-label">Submit Button</label>
                                              <div class="col-sm-10">
                                                <button type="submit" class="btn btn-primary">Submit</button>
                                              </div>
                                            </div>
                            
                                          </form>
                                      <%}%>  
                                      
                                </div>
                              </div>
                            </div>
                          </div>
                        </div>
                      </section>
                </main>



                <%- include("../../partials/script.ejs") %>
</body>

</html>