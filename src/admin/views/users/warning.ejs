<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>view-saloon</title>
    <%- include('../../partials/headlink.ejs') %>
</head>

<body>
    <%- include('../../partials/Header.ejs') %>
        <%- include("../../partials/sidebar.ejs") %>

            <main id="main" class="main">
                <div class="pagetitle">

                    <h1><%-user?.type %></h1>
                    <nav>
                        <ol class="breadcrumb">
                            <li class="breadcrumb-item"><a href="/">Home</a></li>
                            <li class="breadcrumb-item active">Dashboard</li>
                        </ol>
                    </nav>
                </div>
                <!-- End Page Title -->
                <section class="section dashboard">
                    <div class="row">
                        <div class="row">
                            <div class="col-lg-12">



                                <div class="card">
                                    <div class="card-body">
                                        <h5 class="card-title">Filter</h5>

                                        <!-- No Labels Form -->
                                        <form class="row g-3" action="/warning" method="post">
                                            <div class="col-md-6">
                                                <label>To</label> <br>
                                                <input type="text" class="form-control" placeholder="Name" name="email"
                                                    value=<%=searchobj?.name%>>
                                            </div>
                                            <div class="col-md-6">
                                                <label>Subject</label> <br>
                                                <input type="text" class="form-control" placeholder="Email" name="subject"
                                                    value=<%=searchobj?.email%>>
                                            </div>
                                            <div class="col-md-6">
                                                <label>Message</label> <br>
                                                <textarea name="text" id="" cols="30" rows="10" class="form-control"></textarea>
                                            </div>
                                            
                                            <div class="col-md-4">

                                                <div class="text-center">
                                                    <label></label> <br>
                                                    <button type="submit" class="btn btn-primary">Send</button>
                                                    <a href="/get-All-order"> <button type="button"
                                                            class="btn btn-secondary">Reset</button></a>
                                                </div>
                                            </div>
                                        </form>

                                    </div>
                                </div>




                                
                            </div>
                        </div>

                    </div>
                </section>


            </main>



            <div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel"
                aria-hidden="true">
                <div class="modal-dialog">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="exampleModalLabel">User Service Data</h5>
                            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                        </div>
                        <div class="modal-body" id="moduleDataAppend">
                            ...
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                            <!-- <button type="button" class="btn btn-primary">Save changes</button> -->
                        </div>
                    </div>
                </div>
            </div>


            
            <%- include("../../partials/script.ejs") %>
</body>

</html>